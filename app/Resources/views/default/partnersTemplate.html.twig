<section class="call-to-action" data-aos="fade-zoom-in"
         data-aos-offset="200" data-aos-easing="ease-in-sine"
         data-aos-duration="600" >
    <div class="container">
        <h1 class="display-3">Partners</h1>
        <p class="lead"></p>
    </div>
</section>


<section class="call-to-action alturaMinimaPartners" data-aos="fade-zoom-in"
         data-aos-offset="200" data-aos-easing="ease-in-sine"
         data-aos-duration="600">
    <div class="container">
        <div class="row" style="margin:7%;">
            {% for part in partners %}
                {% if part == partners | last %}
                    <div class="col-xs-12 col-md-6 col-lg-4 img-responsivePartner" data-aos="fade-left" style="min-height:150px;">
                        <img class="img_partner" src="{{ asset(part.rutaimagen) }}"  alt="{{ part.nombre }}"/>
                    </div>
                {% else %}
                    <div class="col-xs-12 col-md-6 col-lg-4 img-responsivePartner" data-aos="fade-left">
                        <img class="img_partner" src="{{ asset(part.rutaimagen) }}"  alt="{{ part.nombre }}"/>
                    </div>
                {% endif %}
            {% endfor %}
        </div>	

    </div>
</section>