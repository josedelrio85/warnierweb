<section class="call-to-action" data-aos="fade-zoom-in"
	data-aos-offset="200" data-aos-easing="ease-in-sine"
	data-aos-duration="600">
	<div class="container">
    	<h1 class="display-3">Clientes</h1>
    	<p class="lead">Algunas de las empresas que confian en nosotros.</p>
	</div>
</section>

<section class="call-to-action alturaMinima" data-aos="fade-zoom-in"
	data-aos-offset="200" data-aos-easing="ease-in-sine" data-aos-duration="600">
	<div class="container">
		<div class="row" style="margin:7%;">
		{% for cli in clientes %}
			{% if cli == clientes | last %}
				<div class="col-xs-12 col-md-6 col-lg-4 img-responsive" data-aos="fade-left" style="min-height:150px;">
					<img class="img_Cliente" src="{{ asset(cli.rutaimagen) }}"  alt="{{ cli.nombre }}"/>
				</div>
			{% else %}
				<div class="col-xs-12 col-md-6 col-lg-4 img-responsive" data-aos="fade-left">
					<img class="img_Cliente" src="{{ asset(cli.rutaimagen) }}"  alt="{{ cli.nombre }}"/>
				</div>
			{% endif %}
		{% endfor %}
		</div>
	</div>
</section>