<?php

namespace AppBundle\Entity;

/**
 * Seccion
 */
class Seccion
{
    /**
     * @var string
     */
    private $titulo;

    /**
     * @var boolean
     */
    private $activo;

    /**
     * @var integer
     */
    private $id;


    /**
     * Set titulo
     *
     * @param string $titulo
     *
     * @return Seccion
     */
    public function setTitulo($titulo)
    {
        $this->titulo = $titulo;

        return $this;
    }

    /**
     * Get titulo
     *
     * @return string
     */
    public function getTitulo()
    {
        return $this->titulo;
    }

    /**
     * Set activo
     *
     * @param boolean $activo
     *
     * @return Seccion
     */
    public function setActivo($activo)
    {
        $this->activo = $activo;

        return $this;
    }

    /**
     * Get activo
     *
     * @return boolean
     */
    public function getActivo()
    {
        return $this->activo;
    }

    /**
     * Get id
     *
     * @return integer
     */
    public function getId()
    {
        return $this->id;
    }
}
